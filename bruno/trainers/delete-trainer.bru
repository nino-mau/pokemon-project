meta {
  name: Delete Trainer
  type: http
  seq: 5
}

delete {
  url: http://localhost:3000/api/trainers/{{trainerId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
}

tests {
  test("Deletes trainer successfully", function() {
    const data = res.getBody();
    expect(data).to.be.an("object");
    expect(data.message).to.equal("Trainer deleted successfully");
  });
}
