meta {
  name: Delete Gym
  type: http
  seq: 5
}

delete {
  url: http://localhost:3000/api/gyms/{{gymId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
}

tests {
  test("Deletes gym successfully", function() {
    const data = res.getBody();
    expect(data).to.be.an("object");
    expect(data.message).to.equal("Gym deleted successfully");
  });
}
